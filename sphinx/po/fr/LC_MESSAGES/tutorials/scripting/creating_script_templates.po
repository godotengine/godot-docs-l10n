msgstr "Valeur"

msgid "``%INT_TYPE%``"
msgstr "``%INT_TYPE%``"

msgid "``: int``"
msgstr "``: int``"

msgid "``%STRING_TYPE%``"
msgstr "``%STRING_TYPE%``"

msgid "``: String``"
msgstr "``: String``"

msgid "``%FLOAT_TYPE%``"
msgstr "``%FLOAT_TYPE%``"

msgid "``: float``"
msgstr "``: float``"

