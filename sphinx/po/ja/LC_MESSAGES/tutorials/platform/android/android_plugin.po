msgstr "動画広告"

msgid "Cross-promotion"
msgstr "クロスプロモーション"

msgid "In-game soft & hard currencies"
msgstr "ゲーム内のソフト通貨とハード通貨"

msgid "Promo codes"
msgstr "プロモーションコード"

msgid "A/B testing"
msgstr "A/B テスト"

msgid "Login"
msgstr "ログイン"

msgid "Cloud saves"
msgstr "クラウド保存"

msgid "Leaderboards and scores"
msgstr "ランキングとスコア"

msgid "User support & feedback"
msgstr "ユーザーサポートとフィードバック"

